{
    "name": "zendframework/zend-expressive-twigrenderer",
    "description": "Twig integration for Expressive",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "expressive",
        "http",
        "middleware",
        "psr",
        "psr-7",
        "twig"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev",
            "dev-develop": "1.5-dev"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "container-interop/container-interop": "^1.2",
        "twig/twig": "^1.32 || ^2.1",
        "zendframework/zend-expressive-helpers": "^1.4 || ^2.2 || ^3.0.1 || ^4.0",
        "zendframework/zend-expressive-router": "^1.3.2 || ^2.1",
        "zendframework/zend-expressive-template": "^1.0.4"
    },
    "require-dev": {
        "malukenho/docheader": "^0.1.5",
        "phpunit/phpunit": "^6.0.7 || ^5.7.14",
        "zendframework/zend-coding-standard": "~1.0.0"
    },
    "autoload": {
      "psr-4": {
        "Zend\\Expressive\\Twig\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "ZendTest\\Expressive\\Twig\\": "test/"
      }
    },
    "suggest": {
        "mouf/pimple-interop": "^1.0 to use Pimple for dependency injection",
        "aura/di": "^3.2 to make use of Aura.Di dependency injection container",
        "zendframework/zend-servicemanager": "^3.3 to use zend-servicemanager for dependency injection"
    },
    "scripts": {
        "check": [
            "@license-check",
            "@cs-check",
            "@test"
        ],
        "upload-coverage": "coveralls -v",
        "cs-check": "phpcs --colors",
        "cs-fix": "phpcbf --colors",
        "license-check": "docheader check src/ test/",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    }
}
