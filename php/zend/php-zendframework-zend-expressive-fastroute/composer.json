{
    "name": "zendframework/zend-expressive-fastroute",
    "description": "FastRoute integration for Expressive",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "expressive",
        "FastRoute",
        "http",
        "middleware",
        "psr",
        "psr-7"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-develop": "2.1-dev"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "nikic/fast-route": "^1.0.0",
        "psr/http-message": "^1.0",
        "zendframework/zend-expressive-router": "^2.0",
        "fig/http-message-util": "^1.1",
        "zendframework/zend-stdlib": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7 || ^5.6",
        "zendframework/zend-coding-standard": "~1.0.0",
        "malukenho/docheader": "^0.1.5"
    },
    "autoload": {
      "psr-4": {
        "Zend\\Expressive\\Router\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "ZendTest\\Expressive\\Router\\": "test/"
      }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@license-check",
            "@test"
        ],
        "coveralls": "coveralls",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "license-check": "docheader check src/ test/",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    }
}
