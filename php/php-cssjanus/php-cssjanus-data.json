			[
				".foo { background-position: 10px 40%; }"
			],
			[
				".foo { background-position: 10px 2.3%; }"
			]
		]
	},
	"flip background percentages": {
		"cases": [
			[
				".foo { background: url(/foo/bar.png) 77% 40%; }",
				".foo { background: url(/foo/bar.png) 23% 40%; }"
			],
