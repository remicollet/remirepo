	"do not flip rules or properties with @noflip comments": {
		"cases": [
			[
				"/* @noflip */ div { float: left; }"
			],
			[
				"/*! @noflip */ div { float: left; }"
			],
			[
				"/* @noflip */ div, .notme { float: left; }"
			],
			[
				"/* @noflip */ div { float: left; } div { float: right; }",
