				"/* @noflip */\ndiv { float: left; }\ndiv { float: right; }",
				"/* @noflip */\ndiv { float: left; }\ndiv { float: left; }"
			],
			[
				"div { float: right; /* @noflip */ float: left; }",
				"div { float: left; /* @noflip */ float: left; }"
			],
			[
				"div\n{ float: right;\n/* @noflip */\n float: left;\n; }",
