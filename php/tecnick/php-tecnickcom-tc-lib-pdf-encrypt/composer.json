{
    "name": "tecnickcom/tc-lib-pdf-encrypt",
    "description": "PHP library to encrypt data for PDF",
    "type": "library",
    "homepage": "http://www.tecnick.com",
    "license": "LGPL-3.0",
    "keywords": ["tc-lib-pdf-encrypt", "PDF", "encrypt", "encryption", "AES", "RC4"],
    "authors": [
        {
            "name": "Nicola Asuni",
            "email": "info@tecnick.com",
            "role": "lead"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-date": "*",
        "ext-hash": "*",
        "ext-mcrypt": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-posix": "*"
    },
    "require-dev": {
        "apigen/apigen" : "^4.1.2",
        "bartlett/php-compatinfo" : "^5.0.0",
        "pdepend/pdepend" : "^2.2.2",
        "phploc/phploc" : "^2.1.5|^3.0.0",
        "phpmd/phpmd" : "^2.3.2",
        "phpunit/phpunit": "4.8.19|^5.0.0",
        "sebastian/phpcpd" : "^2.0.2",
        "squizlabs/php_codesniffer": "^2.5.0|^3.0.0"
    },
    "autoload": {
        "psr-4":{
            "Com\\Tecnick\\Pdf\\Encrypt\\": "src"
        }
    }
}
