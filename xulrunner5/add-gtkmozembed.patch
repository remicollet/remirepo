+{
+  g_return_val_if_fail(webBrowser, NS_ERROR_FAILURE);
+  nsCOMPtr<nsIDocShell> docShell(do_GetInterface((nsISupports*)webBrowser));
+  NS_ENSURE_TRUE(docShell, NS_ERROR_FAILURE);
+  return docShell->GetContentViewer(aViewer);
+}
