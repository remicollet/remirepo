+    prompter.SetPassword(*aPassword);
+    if (aCheckMsg)
+        prompter.SetCheckMessage(aCheckMsg);
+    if (aCheckValue)
+        prompter.SetCheckValue(*aCheckValue);
+
+    prompter.Create(EmbedPrompter::TYPE_PROMPT_USER_PASS,
