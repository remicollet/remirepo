+    void     Run(void);
+
+private:
+
+    void     SaveDialogValues();
+
+    nsCString    mTitle;
+    nsCString    mMessageText;
+    nsCString    mTextValue;
+    nsCString    mCheckMessage;
+    PRBool       mCheckValue;
